---
batch_connect:
  template: vnc
script:
  native:
    - "-N"
    - "1"
    - "-n"
    - "1" 
    - "-c"
    - "<%= num_cores %>"
    <%- if memtask != "default" -%>
    - "--mem"
    - "<%= memtask %>"
    <%- end -%>
    <%- if reservation.length >0  -%>
    - "--reservation=<%= reservation %>"
    <%- end -%>
